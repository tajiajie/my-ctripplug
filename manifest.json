{
    "manifest_version": 2,
    "name": "ctripPlug",
    "version": "3.0",
    "description": "reload for customization",
    "browser_action": {
      "default_icon": {
          "19": "./xml.jpg"
          },
      "default_title": "携程抢单器",
      "default_popup":"popup.html"
    },
    "permissions": ["<all_urls>"
   // "http://*/",
   // "bookmarks",
   // "tabs",
  //  "history"
    ] ,
    "content_scripts": [
        {
            "matches": ["http://*/*","https://*/*"],
            "js": ["jquery-3.2.1.min.js", "plugin.js", "ctrip.js", "number.js"]
        }
    ]
}